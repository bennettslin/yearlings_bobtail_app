{
  "name": "yearlings_bobtail_app",
  "version": "1.0.0",
  "description": "Website for the Bobtail Yearlings album Yearling's Bobtail.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:delivery": "webpack --mode production --env.admin",
    "deploy": "aws s3 sync build/ s3://yearlingsbobtail",
    "deploy:delivery": "aws s3 sync build__delivery/ s3://yearlingsbobtail--delivery",
    "release": "npm run build && npm run deploy",
    "release:delivery": "npm run build:delivery && npm run deploy:delivery",
    "start": "webpack-dev-server --mode development --env.admin --env.development",
    "lint": "eslint --ext js,jsx app",
    "lint:fix": "eslint --ext js,jsx --fix app",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bennettslin/yearlings_bobtail_app.git"
  },
  "author": "Bobtail Yearlings",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bennettslin/yearlings_bobtail_app/issues"
  },
  "homepage": "https://github.com/bennettslin/yearlings_bobtail_app",
  "jest": {
    "setupFiles": [
      "<rootDir>/app/utils/unitTestSetup"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "debounce": "^1.0.2",
    "email-validator": "^2.0.4",
    "mobile-detect": "^1.4.3",
    "pino": "^5.13.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-audio-player": "^0.10.0",
    "react-dom": "^16.9.0",
    "react-inlinesvg": "^1.1.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-tooltip": "^3.11.1",
    "react-transition-group": "^4.2.2",
    "redux": "^4.0.4",
    "scroll-into-view": "^1.8.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.0.2",
    "date-fns": "^2.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^6.2.2",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "node-sass": "^4.9.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "react-test-renderer": "^16.9.0",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^1.0.0",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
