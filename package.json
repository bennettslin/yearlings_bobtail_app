{
  "name": "yearlings_bobtail_app",
  "version": "1.0.0",
  "description": "Audio webcomic for the album Yearling's Bobtail.",
  "main": "index.js",
  "scripts": {
    "clean": "gatsby clean",
    "build:staging": "gatsby build",
    "build:production": "BUILD=production gatsby build",
    "deploy": "gatsby-plugin-s3 deploy --yes",
    "start": "npm run clean && DESTINATION=local BUILD=runtime gatsby develop --port 1112",
    "serve": "gatsby serve --port 1112",
    "stage": "npm run clean && DESTINATION=local npm run build:staging && npm run serve",
    "release:candidate": "npm run clean && DESTINATION=candidate npm run build:staging && npm run deploy",
    "release:delivery": "npm run clean && DESTINATION=delivery npm run build:production && npm run deploy",
    "release:production": "npm run clean && DESTINATION=production npm run build:production && npm run deploy",
    "lint": "eslint --ext js,jsx admin src",
    "lint:fix": "eslint --ext js,jsx --fix admin src"
  },
  "author": "Bobtail Yearlings",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bennettslin/yearlings_bobtail_app.git"
  },
  "bugs": {
    "url": "https://github.com/bennettslin/yearlings_bobtail_app/issues"
  },
  "homepage": "https://www.yearlingsbobtail.com",
  "dependencies": {
    "bowser": "^2.11.0",
    "classnames": "^2.3.1",
    "email-validator": "^2.0.4",
    "encodeurl": "^1.0.2",
    "prop-types": "^15.7.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-audio-player": "^0.17.0",
    "react-dom": "^17.0.2",
    "react-facebook": "^8.1.4",
    "react-helmet": "^6.1.0",
    "react-inlinesvg": "^2.3.0",
    "react-redux": "^7.2.4",
    "react-tooltip": "^4.2.21",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.1",
    "reselect": "^4.0.0",
    "scroll-into-view": "^1.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "date-fns": "^2.23.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-react": "^7.24.0",
    "esm": "^3.2.25",
    "gatsby": "^4.10.1",
    "gatsby-plugin-google-gtag": "^4.10.0",
    "gatsby-plugin-manifest": "^4.10.1",
    "gatsby-plugin-page-creator": "^4.10.1",
    "gatsby-plugin-postcss": "^5.10.0",
    "gatsby-plugin-react-helmet": "^5.10.0",
    "gatsby-plugin-remove-trailing-slashes": "^4.10.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sass": "^5.10.1",
    "gatsby-plugin-twitter": "^4.10.1",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.27",
    "jsdom": "^16.7.0",
    "postcss": "^8.3.6",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.38.0",
    "slugify": "^1.6.5",
    "webpack": "^5.70.0"
  }
}
