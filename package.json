{
  "name": "yearlings_bobtail_app",
  "version": "1.0.0",
  "description": "Website for the Bobtail Yearlings album Yearling's Bobtail.",
  "main": "index.js",
  "scripts": {
    "start": "DESTINATION=local BUILD=runtime gatsby develop --port 1113",
    "clean": "gatsby clean",
    "build:staging": "gatsby build",
    "build:production": "BUILD=production gatsby build",
    "deploy": "gatsby-plugin-s3 deploy --yes",
    "serve": "gatsby serve --port 1113",
    "stage": "DESTINATION=local npm run build:staging && npm run serve",
    "release:candidate": "npm run clean && DESTINATION=candidate npm run build:staging && npm run deploy",
    "release:delivery": "npm run clean && DESTINATION=delivery npm run build:production && npm run deploy",
    "release:production": "npm run clean && DESTINATION=production npm run build:production && npm run deploy",
    "lint": "eslint --ext js,jsx app",
    "lint:fix": "eslint --ext js,jsx --fix app"
  },
  "author": "Bobtail Yearlings",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bennettslin/yearlings_bobtail_app.git"
  },
  "bugs": {
    "url": "https://github.com/bennettslin/yearlings_bobtail_app/issues"
  },
  "homepage": "https://www.yearlingsbobtail.com",
  "dependencies": {
    "bowser": "^2.11.0",
    "classnames": "^2.3.1",
    "email-validator": "^2.0.4",
    "encodeurl": "^1.0.2",
    "prop-types": "^15.7.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-audio-player": "^0.17.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-inlinesvg": "^2.3.0",
    "react-redux": "^7.2.4",
    "react-tooltip": "^4.2.21",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.1",
    "reselect": "^4.0.0",
    "scroll-into-view": "^1.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-sent": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "date-fns": "^2.23.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "esm": "^3.2.25",
    "gatsby": "^2.32.4",
    "gatsby-plugin-google-gtag": "^3.11.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-page-creator": "^2.10.0",
    "gatsby-plugin-postcss": "^3.7.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.10.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sass": "^3.2.0",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "jsdom": "^16.2.0",
