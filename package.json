{
  "name": "yearlings_bobtail_app",
  "version": "1.0.0",
  "description": "Website for the Bobtail Yearlings album Yearling's Bobtail.",
  "main": "index.js",
  "scripts": {
    "clean": "gatsby clean",
    "start": "YB_ENV=local gatsby develop --port 1113",
    "build:staging": "YB_ENV=staging gatsby build",
    "build:production": "YB_ENV=production gatsby build",
    "serve": "gatsby serve --port 1113",
    "stage": "npm run build:staging && npm run serve",
    "empty:candidate": "aws s3 rm s3://yearlingsbobtail--candidate --recursive",
    "deploy:candidate": "aws s3 sync public/ s3://yearlingsbobtail--candidate",
    "release:candidate": "npm run build:staging && npm run empty:candidate && npm run deploy:candidate",
    "empty:delivery": "aws s3 rm s3://yearlingsbobtail--delivery --recursive",
    "deploy:delivery": "aws s3 sync public/ s3://yearlingsbobtail--delivery",
    "release:delivery": "npm run build:production && npm run empty:delivery && npm run deploy:delivery",
    "empty:production": "aws s3 rm s3://yearlingsbobtail --recursive",
    "deploy:production": "aws s3 sync public/ s3://yearlingsbobtail",
    "release:production": "npm run build:production && npm run empty:production && npm run deploy:production",
    "lint": "eslint --ext js,jsx app",
    "lint:fix": "eslint --ext js,jsx --fix app"
  },
  "author": "Bobtail Yearlings",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bennettslin/yearlings_bobtail_app.git"
  },
  "bugs": {
    "url": "https://github.com/bennettslin/yearlings_bobtail_app/issues"
  },
  "homepage": "https://www.yearlingsbobtail.com",
  "dependencies": {
    "classnames": "^2.2.6",
    "debounce": "^1.2.0",
    "email-validator": "^2.0.4",
    "mobile-detect": "^1.4.4",
    "pino": "^5.16.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^16.12.0",
    "react-audio-player": "^0.13.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-inlinesvg": "^1.2.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-tooltip": "^4.0.3",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "scroll-into-view": "^1.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-sent": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "date-fns": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.18.3",
    "esm": "^3.2.25",
    "file-loader": "^5.1.0",
    "gatsby": "^2.22.9",
    "gatsby-env-variables": "^2.0.0",
    "gatsby-plugin-google-analytics": "^2.3.2",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-postcss": "^2.3.2",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.11",
    "gatsby-plugin-sass": "^2.3.2",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "jsdom": "^16.2.0",
    "node-sass": "^4.14.1",
    "redux-devtools-extension": "^2.13.8",
    "webpack": "^4.41.6"
  }
}
